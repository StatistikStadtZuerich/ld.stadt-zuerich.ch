{
  "baseConfig": "trifid:config-sparql.json",
  "listener": {
       "port": 8080
  },
  "sparqlEndpointUrl": "https://lindas-data.ch:8443/lindas/query",
  "sparqlProxy": {
    "options": {
      "queryOperation": "postQueryUrlencoded",
      "authentication": {
        "user": "public",
        "password": "public"
      }
    }
  },
  "staticFiles": {
    "dataadminFiles": {
      "path": "/",
      "folder": "cwd:public"
    }
  },
  "handler": {
    "options": {
      "authentication": {
        "user": "public",
        "password": "public"
      }
    }
  },
  "renderer": {
    "module": "trifid-renderer-simple",
    "template": "cwd:data/template.html",
    "vocab": "renderer:vocab.json"
  }
}
